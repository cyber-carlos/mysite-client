    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "postcss": "^8.2.5",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.66"
  }
