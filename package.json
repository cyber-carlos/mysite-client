    "dayjs": "^1.10.4",
    "nprogress": "^0.2.0",
    "overlayscrollbars": "^1.13.1",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "@types/color": "^3.0.1",
    "@types/node": "^14.14.25",
    "@types/nprogress": "^0.2.0",
    "@types/overlayscrollbars": "^1.12.0",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vitejs/plugin-vue-jsx": "^1.0.3",
